/* Command file for launching QU/2 from Minta -
 please modify to reflect your configuration */

/* replace the "X:" with the drive you have installed QU/2 on -
 e.g. "C:" */
"D:"

/* replace the "cd path" with the directory you have installed QU/2 to -
 e.g. "cd programs\qu2" */
"cd programs\qu2"

ARG file
address cmd 'qupm "'file'"'

